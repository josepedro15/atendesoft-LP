{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "redirects": [
    {
      "source": "/www/(.*)",
      "destination": "https://atendesoft.com/$1",
      "permanent": true
    }
  ],
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 30
    }
  }
}
